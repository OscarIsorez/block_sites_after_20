{
  "manifest_version": 3,
  "name": "Site Blocker",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "45": "images/logo_v1_white.png"
    }
  },
  "icons": {
    "45": "images/logo_v1_white.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "background.js"
      ]
    }
  ]
}